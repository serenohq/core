{
  "name": "znck/sereno-core",
  "description": "A Static blog generator.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rahul Kadyan",
      "email": "hi@znck.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "Znck\\Sereno\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Znck\\Test\\Sereno\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "friendsofphp/php-cs-fixer": "^1.12"
  },
  "require": {
    "symfony/console": "^3.1",
    "illuminate/view": "^5.3",
    "league/commonmark": "^0.15.1",
    "symfony/debug": "^3.1",
    "symfony/yaml": "^3.1",
    "symfony/event-dispatcher": "^3.1",
    "symfony/process": "^3.1",
    "league/commonmark-extras": "^0.1.2",
    "scrivo/highlight.php": "^9.0",
    "webuni/commonmark-table-extension": "^0.6.0",
    "heyupdate/emoji": "^1.0",
    "webuni/commonmark-attributes-extension": "^0.5.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "scripts": {
    "test": "phpunit"
  },
  "bind": ["sereno"]
}
